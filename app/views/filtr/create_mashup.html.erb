<div class="mashup-filter-container section-container">
  <div class="section-header">
    <h2 class="mashup-filter-header">Mashup Filters</h2>
  </div>
  <div class="section-content">
    <div class="mashup-preview">
      <h3>Preview</h3>
      <canvas id="mashup-preview-canvas" class="mashup-preview-canvas">Canvas is not supported by your browser</canvas>
    </div>
    <div class="mashup-layers">
      <h3>Layers</h3>
      <div class="mashup-layers-container">
        <% for i in 1..10 do %>
        <div class="mashup-layer-filter mashup-layer-filter-<%=i%>">
          <div class="mashup-layer-filter-name"><%="nameeee "*i%></div>
          <img class="mashup-layer-filter-thumbnail" src="/assets/temp.png">
        </div>
        <%end%>
      </div>
    </div>
    <div class="mashup-library">
      <h3>Library</h3>
      <div class="mashup-library-container">
        <% for i in 1..10 do %>
        <div class="mashup-library-filter mashup-library-filter-<%=i%>">
          <div class="mashup-library-filter-name"><%="na "*i%></div>
          <img class="mashup-library-filter-thumbnail" src="/assets/temp.png">
        </div>
        <%end%>
      </div>
    </div>
  </div>
</div>